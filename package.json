    "react-top-loading-bar": "^2.3.1",
    "redux": "^4.2.0",
    "redux-thunk": "^2.4.2",
    "sass": "^1.57.1",
    "slick-carousel": "^1.8.1",
    "@reduxjs/toolkit": "^1.9.1",
    "axios": "^0.24.0",
    "framer-motion": "^8.4.4",
